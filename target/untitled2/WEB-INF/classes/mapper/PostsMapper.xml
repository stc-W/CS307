<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!--名称空间-->
<mapper namespace="mapper.PostsMapper">
    <resultMap id="postsResultMap" type="Posts">
        <result column="posting_time" property="postingTime"></result>
        <result column="posting_country" property="postingCountry"></result>
        <result column="posting_city" property="postingCity"></result>
        <result column="author_name" property="authorName"></result>
        <result column="id" property="postID"></result>
        <result column="content" property="contents"></result>
        <result column="category" property="category"></result>
    </resultMap>
</mapper>